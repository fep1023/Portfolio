{
    "manifest_version": 2,
    
    "name": "Password Generation Chrome Plugin",
    "description": "This extension runs a Javascript password generator.",
    "version": "2.0",
    
    "browser_action": {
        "default_icon": "Key.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
    
}